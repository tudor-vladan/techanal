  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+techanal",
      "url": "/?action=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/trading-analysis",
      "accept": {
        "image/*": [".png", ".jpg", ".jpeg"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "permissions": [
    "notifications",
    "background-sync",
    "storage",
    "camera",
    "microphone"
  ],
  "features": [
    "Cross Platform",
